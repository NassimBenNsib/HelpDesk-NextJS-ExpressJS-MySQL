    "@types/geoip-lite": "^1.4.1",
    "@types/hpp": "^0.2.2",
    "@types/ip": "^1.1.0",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.0",
    "@types/nodemailer": "^6.4.8",
    "@types/passport": "^1.0.12",
