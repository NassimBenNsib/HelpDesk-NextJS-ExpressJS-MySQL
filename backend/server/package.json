{
  "name": "backend",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "[init]": "========================================================================",
    "run:init:full": "npx jest --init && npx prisma init",
    "run:init:database": "npx prisma init",
    "[test]": "========================================================================",
    "run:jest": "jest",
    "run:test:full": "jest --watchAll --verbose --coverage --detectOpenHandles --runInBand",
    "[migrate]": "========================================================================",
    "run:migrate:database": "npx prisma migrate dev --name init",
    "run:migrate:model": "npx prisma generate",
    "run:migrate:full": "npx prisma migrate dev --name init && npx prisma generate",
    "[dev]": "========================================================================",
    "run:dev": "ts-node-dev --transpile-only --respawn src/main.ts",
    "run:dev:full": "npx prisma migrate dev --name init && npx prisma generate && yarn run:dev",
    "[build]": "========================================================================",
    "run:build": "tsc",
    "run:build:full": "tsc",
    "[production]": "========================================================================",
    "run:prod:full": "node dist/main.js",
    "run:prod": "node dist/main.js",
    "[tools]": "========================================================================",
    "run:prisma:studio": "npx prisma studio"
  },
